{
  "name": "swoft/stdlib",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "helper",
    "swoft"
  ],
  "description": "microservice framework base on swoole",
  "license": "Apache-2.0",
  "require": {
    "php": ">7.1.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Stdlib\\": "src/"
    },
    "files": [
      "src/Helper/Functions.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload-dev": {
    "psr-4": {
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml",
    "cs-fix": "./vendor/bin/php-cs-fixer fix $1"
  }
}
